{
    "name": "richard-muvirimi/scrappy-remote",
    "type": "project",
    "description": "Php web scrapper utilizing a different machine for the scrapping.",
    "keywords": [
        "scrapper"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Muvirimi",
            "email": "richard@tyganeutronics.com",
            "homepage": "https://richard.co.zw",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.8",
        "bootstrapdash/skydash-free-bootstrap-admin-template": "1.0",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^10.43",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.9",
        "nuwave/lighthouse": "^6.33",
        "ext-pdo": "*",
        "inpsyde/composer-assets-compiler": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "qc": [
            "@pint",
            "@test"
        ],
        "pint": [
            "pint ."
        ],
        "test": "phpunit",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "composer-asset-compiler": {
            "dependencies": "install",
            "script": "build",
            "auto-run": "true",
            "wipe-node-modules": "false"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "inpsyde/composer-assets-compiler": true,
            "dealerdirect/phpcodesniffer-composer-installer": false
        },
        "platform": {
            "php": "8.1"
        },
        "process-timeout": 0
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bootstrapdash/skydash-free-bootstrap-admin-template",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/bootstrapdash/skydash-free-bootstrap-admin-template",
                    "type": "git",
                    "reference": "main"
                }
            }
        }
    ]
}
